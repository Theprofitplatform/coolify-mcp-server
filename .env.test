# Test Environment Configuration
# This file contains safe defaults for local testing
# DO NOT use production credentials here!

# ==============================================================================
# COOLIFY CONFIGURATION (Test/Mock)
# ==============================================================================

# Use mock URL for unit tests (will be mocked anyway)
COOLIFY_BASE_URL=http://localhost:8000
COOLIFY_TOKEN=test-token-12345

# ==============================================================================
# N8N CONFIGURATION (Test/Mock)
# ==============================================================================

N8N_URL=http://localhost:5678

# ==============================================================================
# QDRANT CONFIGURATION (Test/Mock)
# ==============================================================================

QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_API_KEY=test-api-key

# ==============================================================================
# ADDITIONAL CONFIGURATION
# ==============================================================================

DEBUG=false
COOLIFY_API_TIMEOUT=5000
COOLIFY_API_MAX_RETRIES=2
COOLIFY_API_RETRY_DELAY=100

# ==============================================================================
# TEST ENVIRONMENT NOTES
# ==============================================================================
#
# 1. These are SAFE test values - no real credentials
# 2. Unit tests will mock API calls anyway
# 3. Integration tests will use real .env if available
# 4. Never commit real credentials to any .env file
#
